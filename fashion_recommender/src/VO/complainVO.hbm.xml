<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="VO.ComplainVO" table="complain">
		<id name="complainId" type="int" column="complainId">
			<generator class="increment" />
		</id>
		<property name="complainType">
			<column name="complainType" />
		</property>

		<property name="complainDescription">
			<column name="complainDescription" />
		</property>

		<property name="status">
			<column name="status" />
		</property>	
		
		<many-to-one name="loginVO" class="VO.LoginVO" column="loginID"></many-to-one>
	</class>
</hibernate-mapping>